\n            paginate={paginate}\r\n          />\r\n        </div>\r\n      </div>\r\n      <div className=\"col s12 m4 l4\"></div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default TweetList;\r\n","/mnt/c/Users/john john/Desktop/twitter-app/twitter-app/src/store/Schema/tweet.js",[],"/mnt/c/Users/john john/Desktop/twitter-app/twitter-app/src/components/form/search-form.component.jsx",["124"],"/mnt/c/Users/john john/Desktop/twitter-app/twitter-app/src/components/form/formik-control.js",[],"/mnt/c/Users/john john/Desktop/twitter-app/twitter-app/src/components/form/Input.component.jsx",[],"/mnt/c/Users/john john/Desktop/twitter-app/twitter-app/src/components/form/text-error.component.jsx",[],["125","126"],"/mnt/c/Users/john john/Desktop/twitter-app/twitter-app/src/components/form/Search-form.component.jsx",[],"/mnt/c/Users/john john/Desktop/twitter-app/twitter-app/src/components/Pagination.component.jsx",[],"/mnt/c/Users/john john/Desktop/twitter-app/twitter-app/src/store/action/api.js",[],"/mnt/c/Users/john john/Desktop/twitter-app/twitter-app/src/store/middleware/toast.js",[],"/mnt/c/Users/john john/Desktop/twitter-app/twitter-app/src/utils/toastify.js",[],"/mnt/c/Users/john john/Desktop/twitter-app/twitter-app/src/store/action/websocket.js",[],"/mnt/c/Users/john john/Desktop/twitter-app/twitter-app/src/store/middleware/websocket.js",["127"],"import * as actions from '../action/websocket.js';\r\n\r\nconst websocket = ({ dispatch }) => (next) => async (action) => {\r\n  if (action.type !== actions.webSocketCallBegan.type) return next(action);\r\n  next(action); // 'apiCallBegan' to show in redux dev tools\r\n  const { message, onSuccess, onError } = action.payload;\r\n  try {\r\n    // General\r\n    // dispatch(actions.apiCallSuccess(response.data));\r\n    // Specific\r\n    // console.log('message: ', JSON.parse(message));\r\n    if (onSuccess) dispatch({ type: onSuccess, payload: JSON.parse(message) });\r\n  } catch (error) {\r\n    // console.log('error: ', error